@model IEnumerable<Artist>
@{
    ViewData["Title"] = "Index";
}
<a asp-action="Create">Create an Artist</a>  
<h2>Here is the list of Artists</h2>
<table style="width:50%">
    <thead>
        <tr>
            <th>ArtistId</th>
            <th>Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (Artist e in Model)
        {
            <tr>
                <td>@e.ArtistId</td>
                <td>@e.Name</td>

            </tr>
        }
    </tbody>
</table>

